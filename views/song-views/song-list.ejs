<% include ../partials/header %>

<div class="song-list-wrapper">
  <h2 class="song-list-header">Song Name<div class="song-list-spacer"></div>Song Difficulty</h2>
  <% for (let i = 0; i < data.length; i++) { %>
  <div>
    <a class="song-list-link" href="/songs/<%= data[i]._id%>"><span><%= data[i].name %></span></a>
    <div class="song-list-spacer"></div>
    <div class="difficulty-stars-display">
      <% for (let k = 0; k < data[i].difficulty; k++) { %>
        <img class="star" src="images/filled-star.png" height="30" width="30">
      <% } %>
      <% for (let j = 0; j < 5 - data[i].difficulty; j++) { %>
        <img class="star" src="images/empty-star.png" height="30" width="30">
    <% } %>
    </div>
  </div>
  <% } %>
